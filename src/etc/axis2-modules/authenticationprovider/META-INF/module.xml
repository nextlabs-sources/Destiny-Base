<module name="authentication-provider" class="com.bluejungle.destiny.appframework.appsecurity.axis.AuthenticationHandlerModule">
    <InFlow>
        <handler name="InFlowEnrollmentHandler" class="com.bluejungle.destiny.appframework.appsecurity.axis.AuthenticationHandler">
            <order phase="authenticationHandler"/>
        </handler>
    </InFlow>
    <InFlowFault>
        <handler name="FaultInFlowEnrollmentHandler" class="com.bluejungle.destiny.appframework.appsecurity.axis.AuthenticationHandler">
            <order phase="authenticationHandler"/>
        </handler>
    </InFlowFault>
    <OutFlow>
        <handler name="OutFlowEnrollmentHandler" class="com.bluejungle.destiny.appframework.appsecurity.axis.AuthenticationHandler">
            <order phase="authenticationHandler"/>
        </handler>
    </OutFlow>
    <OutFlowFault>
        <handler name="FaultOutFlowEnrollmentHandler" class="com.bluejungle.destiny.appframework.appsecurity.axis.AuthenticationHandler">
            <order phase="authenticationHandler"/>
        </handler>
    </OutFlowFault>
</module>
