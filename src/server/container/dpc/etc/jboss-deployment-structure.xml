<?xml version="1.0" encoding="UTF-8"?>
<jboss-deployment-structure>
    <deployment>
    	<exclusions>
			<module slot="main" name="com.fasterxml.jackson.core.jackson-databind"/>
			<module slot="main" name="com.fasterxml.jackson.core.jackson-annotations"/>
			<module slot="main" name="com.fasterxml.jackson.core.jackson-core"/>
			<module slot="main" name="com.fasterxml.jackson.jaxrs.jackson-jaxrs-json-provider"/>
			<module slot="main" name="com.microsoft.azure.storage"/>
			<module slot="main" name="org.jboss.resteasy.resteasy-jackson2-provider"/>
            <module slot="main" name="org.codehaus.woodstox"/>
		</exclusions>
        <dependencies>
            <module name="com.nextlabs.plugin.lib" optional="true" export="false"/>
        </dependencies>
    </deployment>
</jboss-deployment-structure>

